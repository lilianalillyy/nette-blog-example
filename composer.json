{
  "name": "lilianalillyy/nubium-trial",
  "description": "Nubium trial task",
  "type": "project",
  "require": {
    "php": ">= 8.1",
    "nette/application": "^3.1",
    "contributte/bootstrap": "^0.5",
    "nette/caching": "^3.1",
    "nette/database": "^3.1",
    "nette/di": "^3.0",
    "nette/finder": "^2.5",
    "nette/forms": "3.1.7",
    "nette/http": "^3.1",
    "nette/mail": "^3.1",
    "nette/robot-loader": "^3.3",
    "nette/security": "^3.1",
    "nette/utils": "^3.2",
    "latte/latte": "^2.9",
    "tracy/tracy": "^2.8",
    "contributte/forms-bootstrap": "^0.5.3",
    "contributte/neonizer": "^0.5.0"
  },
  "scripts": {
    "post-install-cmd": [
      "Contributte\\Neonizer\\NeonizerExtension::process"
    ],
    "post-update-cmd": [
      "Contributte\\Neonizer\\NeonizerExtension::process"
    ]
  },
  "require-dev": {
    "nette/tester": "^2.3",
    "symfony/thanks": "^1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "symfony/thanks": true
    }
  },
  "extra": {
    "neonizer": {
      "files": [
        {
          "dist-file": "config/local.neon.dist"
        }
      ]
    }
  }
}
